{
    "name": "sinask/laravel-api",
    "description": "Laravel wrapper for the Uptime Kuma API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "UptimeKuma\\LaravelApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UptimeKuma\\LaravelApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UptimeKuma\\LaravelApi\\UptimeKumaServiceProvider"
            ],
            "aliases": {
                "UptimeKuma": "UptimeKuma\\LaravelApi\\Facades\\UptimeKuma"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
